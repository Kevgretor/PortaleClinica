<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
        version="2.0">
	<persistence-unit name="clinica-unit" transaction-type="JTA">
	<provider>org.hibernate.ejb.HibernatePersistence</provider>
	<jta-data-source>clinica-unit-db</jta-data-source>
		<class>it.uniroma3.persistence.model.Medico</class>
		<class>it.uniroma3.persistence.model.Utente</class>
		<class>it.uniroma3.persistence.model.TipologiaEsame</class>
		<class>it.uniroma3.persistence.model.Esame</class>
	</persistence-unit>
</persistence>